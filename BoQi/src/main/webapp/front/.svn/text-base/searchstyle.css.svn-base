@charset "utf-8";
/* zySearch */
.zySearch{position:relative;width:520px;height:35px; float:left;margin-top:15px; margin-left:30px;}

.zySearch .search-input{color:#999;border:1px solid #D0D0D0;height:33px;line-height:33px;margin-right:5px;padding:0 10px;width:300px;}
.zySearch .search-btn{background:none repeat scroll 0 0 #9c3; margin-top:10px; border:0 none;border-radius:0;color:#FFFFFF;cursor:pointer;height:35px;line-height:33px;padding:0;vertical-align:baseline !important;width:76px;text-align:center;vertical-align:middle;white-space:nowrap;margin-bottom:0;font-weight:normal;font-size:14px;display:inline-block;position:relative;}
